{
  "name": "Pomodoro Timer",
  "description": "A timer",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Pomodoro Timer"
  },
   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["blocked.js"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
