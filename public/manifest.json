{
  "name": "Focus Haven",
  "description": "A focus assistent",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Focus Haven"
  },
   "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["blocked.js"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
